(: Because the value in the datasets jsons are in a list, this query converts it to a string and ensures a single item in case of multiple matches :)

let $name :=
    (
        for $i in json-file("/data/datasets.jsonl", 10)
        where $i.id eq "https_58__47__47_hdl.handle.net_47_1839_47_00-0000-0000-0009-428D-6"
        return $i.name
    )[[1]]
    
return string($name)
